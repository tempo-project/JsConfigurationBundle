{
    "name": "tempo-project/jsconfiguration-bundle",
    "description": "A pretty nice way to expose your configuration to your JavaScript.",
    "keywords": [ "configuration", "javascript", "symfony" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mlanawo Mbechezi",
            "email": "mlanawo.mbechezi@ikimea.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3",
        "symfony/filesystem": "~2.4",
        "symfony/console": "~2.3"
    },
    "require-dev": {
        "symfony/http-kernel": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/browser-kit": "~2.3"
    },
    "autoload": {
        "psr-4": { "Tempo\\Bundle\\JsConfigurationBundle\\": "" }
    },
    "target-dir": "Tempo/Bundle/JsConfigurationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
